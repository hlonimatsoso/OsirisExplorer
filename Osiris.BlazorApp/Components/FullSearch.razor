@inherits FullSearchBase

<div>
    <h4>Full Search (@Count)</h4>
</div>
<div class="container">
    <div class="item">
        @*<MatTooltip Tooltip="@BreedList">*@
            <MatTextField  @bind-Value="@NameSearchString" OnInput="@(e => NameSearchString = e.Value.ToString())" OnKeyUp="@HandleKeyUp" Label="Name"></MatTextField>

        @*</MatTooltip>*@
