    <div class="item">
        <MatTextField @bind-Value="@BreedSearchString" OnInput="@(e => BreedSearchString = e.Value.ToString())" OnKeyUp="@HandleKeyUp" Label="Breed"></MatTextField>
        @BreedList
    </div>
</div>

